<!DOCTYPE html>
<html>
<head>
	<title>alumnilist</title>
</head>
<body>
	<h1>Alumni List</h1>

	<a href="/home">Back</a> |
	<a href="/logout">logout</a>

<br><br>
	<table border="1">
		<tr>
			<td>User ID</td>
			<td>Name</td>
			<td>User Type</td>
			<td>Designation</td>
			<td>Contactnumber</td>
			<td>Email</td>
			<td>ACTION</td>
		</tr>

	<% for(var i=0; i< user.length; i++){ %>
		<tr>
			<td><%= user[i].userid %></td>
			<td><%= user[i].name %></td>
			<td><%= user[i].usertype %></td>
			<td><%= user[i].designation %></td>
			<td><%= user[i].contactnumber %></td>
			<td><%= user[i].email %></td>
			
			<td>
				<a href="/user/edit/<%= user[i].userid %>"> EDIT </a> | 
				<a href="/user/delete/<%= user[i].userid %>"> DELETE </a> | 
				<a href="/user/details/<%= user[i].userid %>"> Details </a>
			</td>
		</tr>

	<% } %>
	</table>
</body>
</html>